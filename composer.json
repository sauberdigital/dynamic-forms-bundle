{
  "name": "sauberdigital/dynamic-forms-bundle",
  "description": "A powerful Symfony bundle that simplifies the creation of dynamic form fields",
  "type": "symfony-bundle",
  "keywords": [
    "symfony",
    "bundle",
    "forms",
    "dynamic-forms",
    "form-builder",
    "form-type",
    "conditional-fields",
    "dependent-fields"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sauberdigital/dynamic-forms-bundle",
  "support": {
    "issues": "https://github.com/sauberdigital/dynamic-forms-bundle/issues",
    "source": "https://github.com/sauberdigital/dynamic-forms-bundle"
  },
  "authors": [
    {
      "name": "sauber digital, David Schaller",
      "email": "info@sauberdigital.de",
      "homepage": "https://sauberdigital.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "Sd\\DynamicFormsBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sd\\DynamicFormsBundle\\Tests\\": "tests"
    }
  },
  "require": {
    "php": ">=8.2",
    "symfony/form": "^7.2",
    "symfony/http-kernel": "^7.2",
    "symfony/dependency-injection": "^7.2",
    "symfony/event-dispatcher": "^7.2",
    "symfony/options-resolver": "^7.2",
    "symfony/property-access": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "symfony/validator": "^7.2"
  },
  "config": {
    "sort-packages": true
  }
}